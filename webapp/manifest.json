{
  "_version": "1.32.0",
  "sap.app": {
    "id": "com.nttdata.sap.training2022.mog.ca.appdemo1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZGWNTTXX_SERV01_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },

      "jsonData": {
        "uri": "model/CustomData.json", 
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "fullWidth": true, 
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.96.7",
      "libs": {
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.nttdata.sap.training2022.mog.ca.appdemo1.i18n.i18n"
        }
      },
      "myData": {
        "dataSource": "jsonData", 
        "type": "sap.ui.model.json.JSONModel", 
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      }, 
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay", 
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true,
          "groupId": "$direct"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.nttdata.sap.training2022.mog.ca.appdemo1.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "MainPageRoute",
          "pattern": "",
          "target": [
            "MainPage"
          ]
        }, 
        {
          "name": "MainPageRoute2",
          "pattern": "MainPage",
          "target": [
            "MainPage"
          ]
        }, 
        {
          "name": "NextPageRoute",
          "pattern": "NextPage/:companyId:",
          "target": [
            "NextPage"
          ]
        }, 
        {
          "name": "NextPageRoute2",
          "pattern": "NextPage/companyId={companyId}",
          "target": [
            "NextPage"
          ]
        }
      ],
      "targets": {
        "MainPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "s1_MainPage",
          "viewName": "s1_MainPage", 
          "viewLevel": 1
        }, 

        "NextPage": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "s2_NextPage",
          "viewName": "s2_NextPage", 
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "com.nttdata.sap.training2022.mog.ca.appdemo1.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
